@using System.Globalization;
@using Task6.Entities;
@using Task6.Ioc;
@{
    var userLogic = DependencyResolver.UserLogic;
    int awardId = 0;

    if (Request.Cookies["awardId"] != null)
    {
        awardId = Int32.Parse(Request.Cookies["awardId"].Value);

        userLogic.RemoveAwardFromAllUsers(awardId);
    }

    Response.Redirect("delete_award_from_user.cshtml");
}


